{
  "name": "app-scaffold",
  "version": "0.1.0",
  "private": true,
  "packageManager": "npm@10.5.0",
  "scripts": {
    "dev": "next dev --turbopack",
    "build:jobs": "node scripts/compileJobs.mjs",
    "build": "npm run build:jobs && next build",
    "start": "next start",
    "lint": "npm run lint:env && npm run lint:md && biome check .",
    "lint:md": "markdownlint '**/*.md' --ignore node_modules",
    "format": "biome format . --write",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "e2e": "vitest run -c vitest.e2e.config.ts",
    "e2e:smoke": "vitest run -c vitest.e2e.config.ts --testNamePattern \"@smoke\"",
    "reanalyze": "ts-node --transpile-only scripts/updateMissingAnalysis.ts",
    "poll:snailmail": "ts-node --transpile-only scripts/pollSnailMail.ts",
    "scan:inbox": "ts-node --transpile-only scripts/scanInbox.ts",
    "cleanup:anon-cases": "ts-node --transpile-only scripts/cleanupAnonymousCases.ts",
    "migrate": "ts-node --transpile-only scripts/migrate.ts",
    "lint:env": "dotenv-linter .env.example",
    "lint:env:local": "dotenv-linter .env",
    "seed:cases": "ts-node --transpile-only scripts/seedCases.ts",
    "https": "local-ssl-proxy --source 443 --target 3000",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook:screenshot": "npm run build-storybook && storycap http://localhost:6006 --serverCmd \"http-server storybook-static -p 6006\" --out storybook-screenshots",
    "generate:schemas": "ts-to-zod --skipValidation --all && npm run validate:generated",
    "validate:generated": "tsc --noEmit -p tsconfig.generated.json",
    "generate:migrations": "drizzle-kit generate:sqlite",
    "squash:migrations": "ts-node --transpile-only scripts/squashMigrations.ts",
    "website": "ts-node --transpile-only scripts/generateWebsiteImages.ts && eleventy",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@anatine/zod-openapi": "^2.2.8",
    "@auth/core": "^0.34.2",
    "@auth/drizzle-adapter": "^1.9.1",
    "@floating-ui/dom": "^1.6.1",
    "@floating-ui/react": "^0.27.12",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-popover": "^1.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-tabs": "^1.0.0",
    "@radix-ui/react-tooltip": "^1.1.7",
    "@react-aria/combobox": "^3.12.5",
    "@react-aria/listbox": "^3.14.6",
    "@react-aria/textfield": "^3.17.5",
    "@react-stately/combobox": "^3.10.6",
    "@tanstack/react-query": "^5.81.2",
    "@tanstack/react-query-devtools": "^5.81.2",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-backend-wasm": "^4.22.0",
    "better-sqlite3": "^11.10.0",
    "casbin": "^5.38.0",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.44.2",
    "exif-parser": "^0.1.12",
    "form-data": "^4.0.0",
    "i18next": "^25.2.1",
    "imapflow": "^1.0.188",
    "is-mobile": "^5.0.0",
    "jsdom": "^26.1.0",
    "mailparser": "^3.7.3",
    "next": "15.3.3",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "onnxruntime-web": "^1.19.0",
    "openai": "^5.2.0",
    "pdf-lib": "^1.17.1",
    "pigeon-maps": "^0.22.1",
    "react": "^19.0.0",
    "react-aria": "^3.41.1",
    "react-dom": "^19.0.0",
    "react-hot-toast": "^2.5.2",
    "react-i18next": "^15.5.3",
    "react-icons": "^4.12.0",
    "react-mermaid2": "^0.1.4",
    "react-zoom-pan-pinch": "^3.7.0",
    "sharp": "^0.34.2",
    "twilio": "^4.23.0",
    "unleash-client": "^6.6.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@axe-core/cli": "^4.10.2",
    "@biomejs/biome": "^1.9.4",
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^8.4.1",
    "@openapitools/openapi-generator-cli": "^2.20.2",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/nextjs": "^8.6.14",
    "@storybook/testing-library": "^0.2.2",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@ts-rest/core": "^3.52.1",
    "@ts-rest/open-api": "^3.52.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-i18next": "^7.8.3",
    "@vitejs/plugin-react": "^4.5.1",
    "@vitest/coverage-v8": "^3.2.3",
    "commander": "^11.1.0",
    "dotenv-linter": "^0.2.0",
    "drizzle-kit": "^0.31.1",
    "esbuild": "^0.25.5",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "http-server": "^14.1.1",
    "local-ssl-proxy": "^2.0.5",
    "markdownlint-cli": "^0.45.0",
    "storycap": "^3.1.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "ts-to-zod": "^3.15.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^3.2.3"
  },
  "optionalDependencies": {
    "@biomejs/cli-linux-x64": "^1.9.4",
    "@rollup/rollup-linux-x64-gnu": "^4.44.1",
    "@tailwindcss/oxide-linux-x64-gnu": "^4.1.10"
  }
}
