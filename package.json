{
  "name": "app-scaffold",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "biome check .",
    "format": "biome format . --write",
    "test": "vitest run",
    "e2e": "vitest run -c vitest.e2e.config.ts",
    "reanalyze": "ts-node --transpile-only scripts/updateMissingAnalysis.ts",
    "poll:snailmail": "ts-node --transpile-only scripts/pollSnailMail.ts",
    "scan:inbox": "ts-node --transpile-only scripts/scanInbox.ts",
    "migrate": "ts-node --transpile-only scripts/migrate.ts",
    "seed:cases": "ts-node --transpile-only scripts/seedCases.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "storybook:screenshot": "npm run build-storybook && storycap http://localhost:6006 --serverCmd \"http-server storybook-static -p 6006\" --out storybook-screenshots",
    "generate:schemas": "ts-to-zod --skipValidation --all && npm run validate:generated",
    "validate:generated": "tsc --noEmit -p tsconfig.generated.json",
    "generate:migrations": "drizzle-kit generate:sqlite",
    "website": "ts-node --transpile-only scripts/generateWebsiteImages.ts && eleventy",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@tippyjs/react": "^4.2.6",
    "better-sqlite3": "^11.10.0",
    "bree": "^9.2.4",
    "commander": "^11.1.0",
    "dotenv": "^16.5.0",
    "exif-parser": "^0.1.12",
    "imapflow": "^1.0.188",
    "leaflet": "^1.9.4",
    "lodash": "4.17.21",
    "mailparser": "^3.7.3",
    "mermaid": "^10.9.3",
    "next": "15.3.3",
    "nodemailer": "^7.0.3",
    "openai": "^5.2.0",
    "pdf-lib": "^1.17.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^4.12.0",
    "react-leaflet": "^5.0.0",
    "react-mermaid2": "^0.1.4",
    "sharp": "^0.34.2",
    "tippy.js": "^6.3.7",
    "ts-node": "^10.9.2",
    "twilio": "^4.23.0",
    "umzug": "^3.8.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0",
    "@anatine/zod-openapi": "^2.2.8",
    "@biomejs/biome": "^1.9.4",
    "@eslint/eslintrc": "^3",
    "@openapitools/openapi-generator-cli": "^2.20.2",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/nextjs": "^8.6.14",
    "@storybook/testing-library": "^0.2.2",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@ts-rest/core": "^3.52.1",
    "@ts-rest/open-api": "^3.52.1",
    "@types/leaflet": "^1.9.18",
    "@types/lodash": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.5.1",
    "@vitest/coverage-v8": "^3.2.3",
    "drizzle-kit": "^0.31.1",
    "drizzle-orm": "^0.44.2",
    "eslint": "^9",
    "eslint-config-next": "15.3.3",
    "http-server": "^14.1.1",
    "jsdom": "^26.1.0",
    "storycap": "^3.1.0",
    "tailwindcss": "^4",
    "ts-to-zod": "^3.15.0",
    "typescript": "^5",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^3.2.3"
  },
  "optionalDependencies": {
    "@biomejs/cli-linux-x64": "^1.9.4",
    "@rollup/rollup-linux-x64-gnu": "^4.43.0",
    "@tailwindcss/oxide-linux-x64-gnu": "^4.1.10",
    "lightningcss-linux-x64-gnu": "^1.30.1"
  }
}
